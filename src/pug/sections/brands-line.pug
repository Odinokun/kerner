mixin brands-line__item(logo)
  li.brands-line__item
    a(href="#").brands-line__link
      img(src="assets/img/brands/" + logo + ".png" alt="logo")

//begin brands-line
.brands-line
  .container
    ul.brands-line__list
      +brands-line__item('bohre')
      +brands-line__item('vessel')
      +brands-line__item('euroboor')
      +brands-line__item('karnasch')
//end brands-line